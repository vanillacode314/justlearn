    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.49.10",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "unplugin-icons": "^0.14.1"
  },
  "type": "module",
  "dependencies": {
    "dotenv": "^16.0.0",
